package com.axsos.calculator;

public class Calculator {
	private String operation="";
	private double operandOne=0;
	private double num2=0;
	public Calculator() {}
	public Calculator(String operator,double num1,double num2) {
		this.operation=operator;
		this.nuxm1=num1;
		this.num2=num2;
	}

	public double getOperandOne() {
		return operandOne;
	}
	public void setOperandOne(double num1) {
		this.operandOne = num1;
	}
	public double getNum2() {
		return num2;
	}
	public void setNum2(double num2) {
		this.num2 = num2;
	}
	public String getOperation() {
		return operation;
	}
	public void setOperation(String operator) {
		this.operation = operator;
	}
	
	
}
