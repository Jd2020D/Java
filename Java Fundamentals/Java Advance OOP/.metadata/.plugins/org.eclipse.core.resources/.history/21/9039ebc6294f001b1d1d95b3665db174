package rotate;

public class rotate {
	
	public static boolean isRotation(String str1,String str2) {
		if(str1.length()!=str2.length())
			return false;
		String temp="";
		// temp adi
	
		// str1 dia 
		for (int i = 0; i < str1.length(); i++) {
			if (str1.equals(temp))
				return true	;
			temp+=str1.charAt(str1.length()-1)
			
		}
		return false; 
	}
	public static void main(String[] args) {
		
	}

}
